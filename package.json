{
  "name": "@javier.alejandro.castro/react-mde",
  "version": "11.0.9",
  "description": "React Markdown Editor",
  "main": "./lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prepare": "yarn run build",
    "postversion": "yarn run ghpages && git add . && git commit -m 'rebuild docs'",
    "test": "jest",
    "build": "cross-env NODE_ENV=production babel src -d lib",
    "dev": "cross-env NODE_ENV=development npx babel-node ./demo/server.js",
    "ghpages": "webpack --config webpack.config.ghpages.js",
    "cypress:open": "cypress open",
    "lint": "eslint 'src/**'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacargentina/react-mde.git"
  },
  "keywords": [
    "react",
    "component",
    "markdown",
    "editor",
    "text-editor",
    "markdown-editor"
  ],
  "author": "Javier Castro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacargentina/react-mde.git/issues"
  },
  "dependencies": {
