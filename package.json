{
  "name": "@javier.alejandro.castro/react-mde",
  "version": "11.0.15",
  "description": "React Markdown Editor",
  "main": "./lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prepare": "yarn run build",
    "postversion": "yarn run ghpages && git add . && git commit -m 'rebuild docs'",
    "test": "jest",
    "build": "cross-env NODE_ENV=production babel src -d lib --delete-dir-on-start",
    "dev": "cross-env NODE_ENV=development npx babel-node ./demo/server.js",
    "ghpages": "webpack --config webpack.config.ghpages.js",
    "cypress:open": "cypress open",
    "lint": "eslint 'src/**'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacargentina/react-mde.git"
  },
  "keywords": [
    "react",
    "component",
    "markdown",
    "editor",
    "text-editor",
    "markdown-editor"
  ],
  "author": "Javier Castro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacargentina/react-mde.git/issues"
  },
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-flow": "^7.13.13",
    "@babel/preset-react": "^7.13.13",
    "@cypress/webpack-preprocessor": "^5.7.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^7.2.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.16.2",
    "file-loader": "^6.2.0",
    "flow-bin": "^0.149.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "showdown": "^1.8.6",
    "styled-jsx": "^3.4.4",
    "webpack": "^5.36.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-stream": "^6.1.2"
  },
  "homepage": "https://github.com/jacargentina/react-mde.git#readme",
  "devDependencies": {
    "@simbathesailor/use-what-changed": "^2.0.0",
    "@testing-library/react": "^11.2.6"
  }
}
